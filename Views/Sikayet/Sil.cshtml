@model Sikayet

<form method="post">
    <input asp-for="Id" hidden />
    <div class="border p-3 mt-5">
        <div class="row pb-3">

            <h2 class="text-primary">Sikayet Sil</h2>
            </hr>
        </div>

        <div class="mb-3">
            <label asp-for="Ad" class="p-0"></label>
            <input asp-for="Ad" disabled class="form-control" />

        </div>



        <button type="submit" class="btn btn-lg btn-primary" style="width:250px">Sikayet Sil</button>
        <a asp-controller="Sikayet" asp-action="Index" class="btn btn-lg btn-primary" style="width:250px">
            Geri Dön

        </a>

    </div>
</form>
